{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "Starter kit for React & Redux on Babel and Webpack",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",    
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "",
    "test:watch": "",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "start-prod": "babel-node tools/distServer.js",
    "create-stack": "aws cloudformation create-stack --stack-name react-redux-serverless-stack --template-body file://.aws/pipeline.yml --parameters file://.aws/pipeline-params.json --capabilities CAPABILITY_NAMED_IAM",
    "delete-stack": "aws cloudformation delete-stack --stack-name react-redux-serverless-stack",
    "aws:cfm:createWebsite": "aws cloudformation create-stack --stack-name react-redux-serverless-website-stack --template-body file://.aws/website.yml --parameters file://.aws/website-params.json --capabilities CAPABILITY_NAMED_IAM",
    "aws:cfm:deleteWebsite": "aws cloudformation delete-stack --stack-name react-redux-serverless-website-stack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "aws-sdk": "^2.32.0",  
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.17",
    "colors": "^1.1.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.12.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "nock": "^9.1.3",
    "npm-run-all": "^4.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  }
}
